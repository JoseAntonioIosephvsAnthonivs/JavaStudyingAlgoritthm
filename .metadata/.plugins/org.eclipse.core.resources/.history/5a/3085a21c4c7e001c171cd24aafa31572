package com.java;

public class TesteConta {
	public static void main(String[]  args) {
		Conta contaCorrente = new Conta();
		Conta contaPoupanca = new Conta();
		Conta contaInvestimento = new Conta();
	
		contaCorrente.numero = 11;
		contaCorrente.saldo = 20.50;
		contaCorrente.cliente.nome = "José";
		contaCorrente.cliente.idade = "23";
		
		contaPoupanca.numero = 22;
		contaPoupanca.saldo = 40.22;
		contaPoupanca.cliente.nome = "José2";
		contaPoupanca.cliente.idade = "22";
		
		contaInvestimento.numero = 33;
		contaInvestimento.saldo = 41.55;
		contaInvestimento.nome = "José3";
		contaInvestimento.idade = "21";
		
		System.out.println(contaCorrente.numero);
		System.out.println(contaCorrente.saldo);
		System.out.println(contaCorrente.cliente);	
		
	}
}